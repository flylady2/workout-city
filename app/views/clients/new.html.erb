<%= form_for @client do |f| %>
   <%= f.label :username %>
   <%= f.text_field :username %>

   <%= f.label :email %>
   <%= f.email_field :email %>

   <%= f.label :password %>
   <%= f.password_field :password %>

   <%= f.label :age %>
   <%= f.number_field :age %>

   <%= f.submit "Create an Account" %>

<% end %>
